#include<stdio.h>

int mySqrt(int);  // Function prototype

int main()
{
    int a;
    printf("enter the number\n");
    scanf("%d",&a);
    
    printf("sqrt is %d\n",mySqrt(a));
    return 0;
}
int mySqrt(int x) {
   int i=1,res;
    if(x==0)
    return 0;
    for(i=1;i*i<=x;i++)
    {
        res=i;
    }
    return res;

}
