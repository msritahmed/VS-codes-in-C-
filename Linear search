#include<stdio.h>
int main()
{
    int a[100],n,s;
    printf("enter the number of elements\n");
    scanf("%d",&n);

    printf("enter the numbers\n");
    for(int i=0;i<n;i++)
    scanf("%d",&a[i]);
    
    printf("enter the number to search\n");
    scanf("%d",&s);

    for(int i=0;i<n;i++)
    {
        if(a[i]==s)
        {
            printf("FOUND at %d\n",i+1);
            return 0;
        }
    }
    printf("NOT FOUND\n");
    return 0;


}
